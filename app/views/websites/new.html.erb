<div class="row">
    <div class="small-12 columns">
      <% if user_signed_in? %>
        <h1>New website</h1>

        <%= render 'form' %>

        <%= link_to 'Back', websites_path %>
      <% else %>
        <h1> You must be logged in to add websites.</h1>
        <h4> Please go to '/admin' and login to add new website examples.</h4>
      <% end %>
    </div>
</div>